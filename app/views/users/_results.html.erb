<% model_class = User %>
<div class="custom-table">
  <div class="row th">
    <div class="col-md-1"><input name="" id='select-all' type="checkbox" value=""></div>
    <div class="col-md-2"><%= model_class.human_attribute_name(:username) %></div>
    <div class="col-md-3"><%= model_class.human_attribute_name(:status) %></div>
    <div class="col-md-4">Date</div>
    <div class="col-md-1">&nbsp;</div>
  </div><!-- row -->

  <% if @users.present? %>
    <div id="user">
      <%= render @users %>
    </div>
  <%else%>
    <h2 class='text-center'><em>No Record Found</em></h2>  
  <% end %>

</div>