<%- model_class = Attribute -%>
<div class="custom-table">
  <div class="row th">
    <div class="row th">
    <div class="col-md-1" id='select-all'><input name="" type="checkbox" value=""></div>
    <div class="col-md-2"><%= model_class.human_attribute_name(:username) %></div>
    <div class="col-md-2"><%= model_class.human_attribute_name(:email) %></div>
    <div class="col-md-2"><%= model_class.human_attribute_name(:status) %></div>
    <div class="col-md-2"><%= model_class.human_attribute_name(:created_at) %></div>
    <div class="col-md-1">&nbsp;</div>
  </div><!-- row -->

  </div><!-- row -->
  <% unless @users.empty? %>
    <% @users.each do |user| %>
      <div class="row td">
        <div class="col-md-1"><input name="" type="checkbox" value=<%= user.id %> ></div>
        <div class="col-md-2"><%= user.username %>&nbsp;</div>
        <div class="col-md-2"><%= user.email %>&nbsp;</div>
        <div class="col-md-2"><%= display_status(user) %>&nbsp;</div>
        <div class="col-md-2"><%= format_date(user, :created_at) %>&nbsp;</div>
        <div class="col-md-1 text-right"><a href="#" class="anchor-block"><i class="fa fa-ellipsis-v"></i></a></div>
      </div>
      <%= render 'shared/inline_action_link', element: user %>
    <% end %>
  <% else %>
      <h2 class='text-center'><em>No Record Found</em></h2>  
  <% end %>
  <%= render 'shared/bootam_action_link', element: 'users' %>
</div>









