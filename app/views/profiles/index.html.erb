<div class="row">
  <div class="page-title">
    <div class="title_left"><h2>Profile list</h2></div><!-- title_left -->
      <div class="title_right">

      <div class="col-md-12 col-sm-12 col-xs-12 form-group pull-right top_search">
        <% if can? :search, Profile %>       
          <%= render 'shared/keyword_search', :condition => :name_cont %>
       
          <div class="advance-search">
            <%= link_to search_profiles_path do %>
             <i class="fa fa-dot-circle-o"></i>
            <% end %>
          </div> 
        <% end %>
        <% if can? :create, Profile %>
          <div class="custom-add"><%= link_to new_profile_path, :title => "New Profile" do %><i class="fa fa-plus"></i><% end %></div>
        <% end %>  
      </div>

    </div>
      <div class="clearfix"></div>                
  </div><!-- page-title -->
</div><!--row-->
<div id="results">
  <%= render :partial => "results", :locals => {:profiles => @profiles} %>
</div>

<%= render 'shared/bootam_action_link', element: Profile %>
<%= will_paginate @profiles , renderer: BootstrapPagination::Rails%>
